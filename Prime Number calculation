#include <iostream>
using namespace std;

bool isprime(int num){
    if(num <= 1)
    return false ;
    for(int i=2 ; i<num ; i++)
        if(num % i == 0)
          return false;
    else return true ;     
}

int main() {
	int num;
	cout<< "enter the number: \n" ;
	cin >> num ;
	isprime(num) ? cout << "TRUE\n" : cout << "FALSE\n" ;
}
