#include <iostream>
#include<string.h> 
using namespace std;

int ispalindrome(char str[]){
    int l = 0 ;
    int h = strlen(str) - 1 ;
    while(l < h){
        if(str[l++] != str[h--]){
            cout << "sting is not palindrome" << "\n" ;
            return 0 ;
        }
        
        
    }
    cout  << "string is palindrome" << "\n";
}

int main() {
	ispalindrome("abba") ;
	ispalindrome("aabbcc");
	return 0;
}
