#include <iostream>
using namespace std;

void fib(int num)
{
    int first = 0 , second = 1, third ;
    for(int i=0 ; i < num ; i++ ){
        cout<< first << " " ;
        third = first + second ;
        first = second;
        second = third;
        
    }
    
}   

int main() {
    int num;
   cout << "Enter the number : ";
   cin >> num;
   cout << "\nThe fibonacci series : " ;
   fib(num);
   return 0;
	
}
